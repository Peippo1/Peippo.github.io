{
  "name": "dequal",
  "version": "2.0.3",
  "description": "A tiny (304B to 489B) utility for check for deep equality",
  "license": "MIT",
  "author": "Luke Edwards <luke.edwards05@gmail.com> (https://lukeed.com)",
  "repository": "lukeed/dequal",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./lite": {
      "types": "./index.d.ts",
      "import": "./lite/index.mjs",
      "require": "./lite/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "*.d.ts",
    "dist",
    "lite"
  ],
  "scripts": {
    "build": "bundt",
    "postbuild": "echo \"lite\" | xargs -n1 cp -v index.d.ts",
    "pretest": "npm run build",
    "test": "uvu -r esm test"
  },
  "types": "index.d.ts",
  "devDependencies": {
    "bundt": "1.0.2",
    "esm": "3.2.25",
    "uvu": "0.3.2"
  },
  "keywords": [
    "deep",
    "deep-equal",
    "equality"
  ],
  "engines": {
    "node": ">=6"
  },
  "modes": {
    "lite": "src/lite.js",
    "default": "src/index.js"
  },
  "unpkg": "dist/index.min.js"
}
