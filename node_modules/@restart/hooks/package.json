{
  "name": "@restart/hooks",
  "version": "0.4.16",
  "license": "MIT",
  "author": "Jason Quense <monastic.panic@gmail.com>",
  "homepage": "https://github.com/react-restart/hooks#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jquense/react-common-hooks.git"
  },
  "bugs": {
    "url": "https://github.com/react-restart/hooks/issues"
  },
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    },
    "./*": {
      "types": "./esm/*.d.ts",
      "import": "./esm/*.js",
      "require": "./cjs/*.js"
    }
  },
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "cjs/index.d.ts",
  "dependencies": {
    "dequal": "^2.0.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "lib"
  },
  "jest": {
    "preset": "@4c",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "release": {
    "conventionalCommits": true
  }
}
